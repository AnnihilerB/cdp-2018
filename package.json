{
  "name": "cdp-2018",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon src/app.js localhost 3000",
    "test": "date && jest --collectCoverageFrom=src/**.js --detectOpenHandles --forceExit --coverage src",
    "lint": "eslint \"**/*.js\" --fix --max-warnings=0",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner"
  },
  "dependencies": {
    "express": "4.16.4",
    "jsdom": "13.0.0",
    "mariadb": "2.0.0-alpha",
    "nodemon": "1.18.6",
    "sonar-scanner": "3.1.0"
  },
  "devDependencies": {
    "eslint": "5.8.0",
    "eslint-config-google": "0.11.0",
    "husky": "1.1.3",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-puppeteer": "3.5.1",
    "puppeteer": "1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint \"**/*.js\" --fix --max-warnings=0"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules/"
    ],
    "coverageReporters": [
      "lcov"
    ]
  }
}
