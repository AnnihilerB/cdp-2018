{
  "name": "cdp-2018",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node src/app.js",
    "test": "jest --detectOpenHandles --forceExit",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage src",
    "e2e": "node src/__tests__/e2e/e2e.js",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner"
  },
  "dependencies": {
    "express": "4.16.4",
    "mariadb": "2.0.0-alpha",
    "eslint": "5.8.0",
    "eslint-config-google": "0.11.0",
    "husky": "1.1.3",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "selenium-webdriver": "4.0.0-alpha.1",
    "sonar-scanner": "3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "eslint \"**/*.js\" --fix --max-warnings=0"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules/",
      "src/__tests__/e2e"
    ]
  }
}
