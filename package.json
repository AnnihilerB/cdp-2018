{
  "name": "cdp-2018",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon src/app.js localhost 3000",
    "test": "date && jest --collectCoverageFrom=src/**.js --detectOpenHandles --forceExit --coverage src"
  },
  "dependencies": {
    "express": "4.16.4",
    "jsdom": "13.0.0",
    "mariadb": "2.0.0-alpha"
  },
  "devDependencies": {
    "eslint": "5.8.0",
    "eslint-config-google": "0.11.0",
    "nodemon": "1.18.6",
    "husky": "1.1.3",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-puppeteer": "3.5.1",
    "puppeteer": "1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "eslint \"**/*.js\" --fix --max-warnings=0"
    }
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "testPathIgnorePatterns": [
      "node_modules/"
    ],
    "coverageReporters": ["lcov"]
  }
}
