{
  "name": "cdp-2018",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon src/app.js localhost 3000",
    "test": "date && jest --detectOpenHandles --forceExit",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage src",
    "e2e": "node src/__tests__/e2e/e2e.js",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner"
  },
  "dependencies": {
    "express": "4.16.4",
    "husky": "1.1.3",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "mariadb": "2.0.0-alpha",
    "nightwatch": "^0.9.21",
    "selenium-webdriver": "4.0.0-alpha.1",
    "jest-junit-reporter": "1.1.0",
    "jsdom": "13.0.0"
  },
  "devDependencies": {
    "eslint": "5.8.0",
    "eslint-config-google": "0.11.0",
    "nodemon": "1.18.6"
  },
  "husky": {
    "hooks": {
      "pre-push": "eslint \"**/*.js\" --fix --max-warnings=0"
    }
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "testPathIgnorePatterns": [
      "node_modules/",
      "src/__tests__/e2e"
    ]
  }
}
