{
  "name": "cdp-2018",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon src/app.js localhost 3000",
    "test": "date && jest --collectCoverageFrom=src/**.js --coverage --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "express": "4.16.4",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-junit-reporter": "1.1.0",
    "jsdom": "13.0.0",
    "mariadb": "2.0.0-alpha",
    "jest-puppeteer": "3.5.1",
    "puppeteer": "1.10.0"
  },
  "devDependencies": {
    "eslint": "5.8.0",
    "eslint-config-google": "0.11.0",
    "nodemon": "1.18.6",
    "husky": "1.1.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "eslint \"**/*.js\" --fix --max-warnings=0 && npm test"
    }
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "testPathIgnorePatterns": [
      "node_modules/"
    ]
  }
}
